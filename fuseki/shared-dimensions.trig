graph <https://lindas.admin.ch/cube/dimension> {
  <technologies>
    a schema:DefinedTermSet, meta:SharedDimension, hydra:Resource, md:SharedDimension ;
    schema:name "Technologies"@en ;
    schema:validFrom "2021-01-20T23:59:59Z"^^xsd:dateTime ;
    sh:property <urn:technologies:defaultMetadata> ;
    schema:additionalProperty <urn:additionalPropery:Order>, <urn:additionalPropery:Color>, <urn:additionalPropery:HelpText> ;
  .

  <urn:additionalPropery:HelpText>
    md:dynamicPropertyType "Lang String" ;
    hydra:required false ;
    rdfs:label "Help text" ;
    rdf:predicate rdfs:comment ;
    sh:languageIn <http://lexvo.org/id/iso639-1/de>, <http://lexvo.org/id/iso639-1/fr>, <http://lexvo.org/id/iso639-1/en> ;
  .

  <urn:shape:additionalProperyHelpText>
    sh:targetNode <urn:additionalPropery:HelpText> ;
    sh:property
      [ sh:path hydra:required ] ,
      [ sh:path rdf:predicate ] ,
      [ sh:path rdfs:label ] ,
      [ sh:path md:dynamicPropertyType ] ,
      [ sh:path sh:languageIn ]
  ;
  .

  <urn:additionalPropery:Order>
    hydra:required false ;
    sh:datatype xsd:integer ;
    rdf:predicate qb:order ;
    rdfs:label "Order" ;
    md:dynamicPropertyType "Literal" ;
  .

  <urn:shape:additionalProperyOrder>
    sh:targetNode <urn:additionalPropery:Order> ;
    sh:property
      [ sh:path hydra:required ] ,
      [ sh:path sh:datatype ] ,
      [ sh:path rdf:predicate ] ,
      [ sh:path rdfs:label ] ,
      [ sh:path md:dynamicPropertyType ]
    ;
   .

  <urn:additionalPropery:Color>
    hydra:required true ;
    sh:class <http://example.com/dimension/colors> ;
    rdf:predicate schema:color ;
    schema:multipleValues true ;
    rdfs:label "Color" ;
    md:dynamicPropertyType "Shared Term" ;
  .

  <urn:shape:additionalProperyColor>
    sh:targetNode <urn:additionalPropery:Color> ;
    sh:property
      [ sh:path hydra:required ] ,
      [ sh:path sh:class ] ,
      [ sh:path rdf:predicate ] ,
      [ sh:path rdfs:label ] ,
      [ sh:path md:dynamicPropertyType ] ,
      [ sh:path schema:multipleValues ]
    ;
   .

  [
    sh:targetNode <technologies> ;
    sh:property
      [ sh:path schema:name ],
      [ sh:path rdf:type ],
      [ sh:path schema:validFrom ] ,
      [ sh:path sh:property ] ,
      [ sh:path schema:additionalProperty ; sh:node <urn:shape:additionalProperyColor> ] ,
      [ sh:path schema:additionalProperty ; sh:node <urn:shape:additionalProperyOrder> ] ,
      [ sh:path schema:additionalProperty ; sh:node <urn:shape:additionalProperyHelpText> ] ;
  ] .

  <technologies/rdf>
    a schema:DefinedTerm, hydra:Resource, md:SharedDimensionTerm ;
    schema:validFrom "2021-01-20T23:59:59Z"^^xsd:dateTime ;
    schema:identifier "rdf" ;
    schema:name "RDF"@en ;
    schema:inDefinedTermSet <technologies> ;
  .

  [
    sh:targetNode <technologies/rdf> ;
    sh:property
      [ sh:path schema:name ],
      [ sh:path rdf:type ],
      [ sh:path schema:validFrom ],
      [ sh:path schema:identifier ],
      [ sh:path schema:inDefinedTermSet ] ;
  ] .

  <technologies/shacl>
    a schema:DefinedTerm, hydra:Resource, md:SharedDimensionTerm ;
    schema:validFrom "2021-01-20T23:59:59Z"^^xsd:dateTime ;
    schema:identifier "shacl", "sh" ;
    schema:name "SHACL"@en ;
    schema:inDefinedTermSet <technologies> ;
  .

  [
    sh:targetNode <technologies/shacl> ;
    sh:property
      [ sh:path schema:name ],
      [ sh:path rdf:type ],
      [ sh:path schema:validFrom ],
      [ sh:path schema:identifier ],
      [ sh:path schema:inDefinedTermSet ] ;
  ] .

  <technologies/sparql>
    a schema:DefinedTerm, hydra:Resource, md:SharedDimensionTerm ;
    schema:validFrom "2021-01-20T23:59:59Z"^^xsd:dateTime ;
    schema:identifier "sparql" ;
    schema:name "SPARQL"@en ;
    schema:inDefinedTermSet <technologies> ;
  .

  [
    sh:targetNode <technologies/sparql> ;
    sh:property
      [ sh:path schema:name ],
      [ sh:path rdf:type ],
      [ sh:path schema:validFrom ],
      [ sh:path schema:identifier ],
      [ sh:path schema:inDefinedTermSet ] ;
  ] .
}
