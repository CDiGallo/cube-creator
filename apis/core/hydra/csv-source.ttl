BASE          <urn:hydra-box:api>
PREFIX cc:    <https://cube-creator.zazuko.com/vocab#>
prefix hydra: <http://www.w3.org/ns/hydra/core#>
prefix code:  <https://code.described.at/>
prefix schema:   <http://schema.org/>

cc:CSVSourceCollection
  a
    hydra:Class ;
  hydra:supportedOperation
    [
      a
        hydra:Operation,
        cc:UploadCSVAction ;
      hydra:title
        "Upload CSV" ;
      hydra:method
        "POST" ;
      code:implementedBy
        [
          a
            code:EcmaScript ;
          code:link
            <file:handlers/csv-source#post> ;
        ] ;
    ] .

cc:CSVSource
  a
    hydra:Class ;
  hydra:supportedOperation
    [
      a
        hydra:Operation,
        cc:GetCSVSourceAction ;
      hydra:title
        "Get CSV Source" ;
      hydra:method
        "GET" ;
      code:implementedBy
        [
          a
            code:EcmaScript ;
          code:link
            <file:handlers/csv-source#get> ;
        ]
    ],
    [
      a
        hydra:Operation,
        schema:DeleteAction ;
      hydra:title
        "Delete CSV Source" ;
      hydra:method
        "DELETE" ;
      code:implementedBy
        [
          a
            code:EcmaScript ;
          code:link
            <file:handlers/csv-source#remove> ;
        ]
    ] .
