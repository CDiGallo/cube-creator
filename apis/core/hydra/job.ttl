prefix code:  <https://code.described.at/>
prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix schema:   <http://schema.org/>

cc:JobCollection
    a
        hydra:Class ;
    hydra:supportedOperation
    [
      a
        hydra:Operation,
        cc:TransformAction ;
