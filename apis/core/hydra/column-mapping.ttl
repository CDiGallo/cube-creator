BASE          <urn:hydra-box:api>
PREFIX cc:    <https://cube-creator.zazuko.com/vocab#>
prefix hydra: <http://www.w3.org/ns/hydra/core#>
prefix code:  <https://code.described.at/>
prefix schema:   <http://schema.org/>
prefix sh:     <http://www.w3.org/ns/shacl#>

cc:LiteralColumnMapping
  a
    hydra:Class ;
  hydra:supportedOperation
  [
    a hydra:Operation,
      schema:UpdateAction ;
    hydra:title
      "Edit column mapping" ;
    hydra:method
      "PUT" ;
    hydra:expects
      cc:ColumnMapping,
      <shape/column-mapping/literal> ;
    code:implementedBy [
      a
        code:EcmaScript ;
      code:link
        <file:handlers/column-mapping#putLiteral> ;
    ]
  ] ,
  [
    a hydra:Operation,
      schema:DeleteAction ;
    hydra:title
      "Delete column mapping" ;
    hydra:method
      "DELETE" ;
    code:implementedBy [
      a
        code:EcmaScript ;
      hydra:expects
        cc:ColumnMapping ;
      code:link
        <file:handlers/column-mapping#remove> ;
    ]
  ] .

cc:ReferenceColumnMapping
  a
    hydra:Class ;
  hydra:supportedOperation
  [
    a hydra:Operation,
      schema:UpdateAction ;
    hydra:title
      "Edit column mapping" ;
    hydra:method
      "PUT" ;
    hydra:expects
      cc:ColumnMapping,
      <shape/column-mapping/reference> ;
    code:implementedBy [
      a
        code:EcmaScript ;
      code:link
        <file:handlers/column-mapping#putReference> ;
    ]
  ] ,
  [
    a hydra:Operation,
      schema:DeleteAction ;
    hydra:title
      "Delete column mapping" ;
    hydra:method
      "DELETE" ;
    code:implementedBy [
      a
        code:EcmaScript ;
      hydra:expects
        cc:ColumnMapping ;
      code:link
        <file:handlers/column-mapping#remove> ;
    ]
  ] .

<shape/column-mapping/literal>
  a
    sh:Shape .

<shape/column-mapping/reference>
  a
    sh:Shape .
